    "/api/analytics/log/v2/recipient-certificates": {
      "get": {
        "tags": [
          "AnalyticsLog"
        ],
        "parameters": [
          {
            "name": "appName",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "App-API-Token",
            "in": "header",
            "schema": {
              "maxLength": 64,
              "minLength": 8,
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/x-pem-file": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/x-pem-file": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
    },
