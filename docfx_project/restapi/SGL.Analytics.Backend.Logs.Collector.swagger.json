  "components": {
    "schemas": {
      "DataEncryptionMode": {
        "enum": [
          "AES_256_CCM",
          "Unencrypted"
        ],
        "type": "string"
      },
      "DataKeyInfo": {
        "type": "object",
        "properties": {
          "mode": {
            "$ref": "#/components/schemas/KeyEncryptionMode"
          },
          "encryptedKey": {
            "type": "string",
            "format": "byte",
            "nullable": true
          },
          "messagePublicKey": {
            "type": "string",
            "format": "byte",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "DownstreamLogMetadataDTO": {
        "type": "object",
        "properties": {
          "logFileId": {
            "type": "string",
            "format": "uuid"
          },
          "creationTime": {
            "type": "string",
            "format": "date-time"
          },
          "endTime": {
            "type": "string",
            "format": "date-time"
          },
          "nameSuffix": {
            "maxLength": 16,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "logContentEncoding": {
            "$ref": "#/components/schemas/LogContentEncoding"
          },
          "encryptionInfo": {
            "$ref": "#/components/schemas/EncryptionInfo"
          },
          "userId": {
            "type": "string",
            "format": "uuid"
          },
          "uploadTime": {
            "type": "string",
            "format": "date-time"
          },
          "size": {
            "type": "integer",
            "format": "int64",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "EncryptionInfo": {
        "type": "object",
        "properties": {
          "dataMode": {
            "$ref": "#/components/schemas/DataEncryptionMode"
          },
          "iVs": {
            "type": "array",
            "items": {
              "type": "string",
              "format": "byte"
            },
            "nullable": true
          },
          "dataKeys": {
            "type": "object",
            "additionalProperties": {
              "$ref": "#/components/schemas/DataKeyInfo"
            },
            "nullable": true
          },
          "messagePublicKey": {
            "type": "string",
            "format": "byte",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "KeyEncryptionMode": {
        "enum": [
          "RSA_PKCS1",
          "ECDH_KDF2_SHA256_AES_256_CCM"
        ],
        "type": "string"
      },
      "LogContentEncoding": {
        "enum": [
          "GZipCompressed",
          "Plain"
        ],
        "type": "string"
      },
      "ProblemDetails": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "detail": {
            "type": "string",
            "nullable": true
          },
          "instance": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": { }
      }
    }
  }
}